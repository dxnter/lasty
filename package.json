    "axios": "^0.19.2",
    "canvas": "^2.6.1",
    "chalk": "^3.0.0",
    "cron": "^1.8.2",
    "dayjs": "^1.9.3",
    "discord.js": "^12.4.0",
    "discord.js-commando": "git+https://github.com/discordjs/Commando.git",
    "esm": "^3.2.25",
    "lowdb": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/node": "^14.14.7",
