{
  "name": "lasty",
  "version": "2.4.2",
  "description": "A Discord bot for fetching Last.fm stats",
  "main": "dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxnter/lasty.git"
  },
  "bugs": {
    "url": "https://github.com/dxnter/lasty/issues"
  },
  "scripts": {
